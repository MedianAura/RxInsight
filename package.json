{
  "name": "Insight",
  "productName": "Insight",
  "version": "1.1.0",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:electron": "ng build && electron-forge start -- --debug",
    "start:electron:aot": "ng build --prod && electron-forge start -- --debug",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "postinstall": "node patch.js"
  },
  "config": {
    "forge": {
      "make_targets": {
        "x64": [
          "squirrel"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "asar": true,
        "quiet": true,
        "ignore": [
          ".idea"
        ]
      },
      "electronWinstallerConfig": {
        "name": "Insight"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "Insight"
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/router": "6.0.0",
    "@ngxs/devtools-plugin": "^3.0.1",
    "@ngxs/form-plugin": "^3.0.1",
    "@ngxs/logger-plugin": "^3.0.1",
    "@ngxs/store": "^3.0.1",
    "argparse": "^1.0.10",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "electron-compile": "^6.4.2",
    "electron-squirrel-startup": "^1.0.0",
    "fs": "^0.0.1-security",
    "fs-jetpack": "^1.3.0",
    "jquery": "^3.3.1",
    "js-yaml": "^3.11.0",
    "json-loader": "^0.5.7",
    "ngx-electron": "^1.0.4",
    "regex-match-all": "^1.0.2",
    "rxjs": "6.1.0",
    "sprintf-js": "^1.1.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "^6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@angular/language-service": "6.0.0",
    "@types/bootstrap": "^3.3.39",
    "@types/electron": "^1.6.10",
    "@types/faker": "^4.1.2",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/jquery": "^3.3.1",
    "@types/js-yaml": "^3.11.1",
    "@types/lodash": "^4.14.108",
    "@types/node": "^10.0.8",
    "@types/sprintf-js": "^1.1.0",
    "codelyzer": "^4.0.1",
    "electron-forge": "^5.2.1",
    "electron-prebuilt-compile": "2.0.0",
    "faker": "^4.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
