{
  "name": "Insight",
  "productName": "Insight",
  "version": "2.0.0",
  "license": "MIT",
  "main": "./src/index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:electron": "ng build && electron-forge start -- --debug",
    "start:electron:server": "electron-forge start -- --debug --server",
    "start:electron:aot": "ng build --prod --aot && electron-forge start -- --debug",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "prune": "yarn install --production --ignore-scripts --prefer-offline"
  },
  "config": {
    "forge": {
      "make_targets": {
        "x64": [
          "squirrel"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "asar": false,
        "quiet": true,
        "ignore": [
          ".idea",
          "node_modules",
          "e2e"
        ]
      },
      "electronWinstallerConfig": {
        "name": "Insight"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "Insight"
      }
    }
  },
  "private": true,
  "dependencies": {
    "argparse": "^1.0.10",
    "bootstrap": "^3.3.7",
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "^2.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^1.3.0",
    "fs-jetpack": "^1.3.0",
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "jquery": "^3.3.1",
    "js-yaml": "^3.11.0",
    "json-loader": "^0.5.7",
    "ng2-dragula": "^1.5.0",
    "ngx-electron": "^1.0.4",
    "regex-match-all": "^1.0.2",
    "rxjs": "6.1.0",
    "sprintf-js": "^1.1.1",
    "zone.js": "^0.8.26",
    "@angular/animations": "6.0.2",
    "@angular/common": "6.0.2",
    "@angular/compiler": "6.0.2",
    "@angular/core": "6.0.2",
    "@angular/forms": "6.0.2",
    "@angular/http": "6.0.2",
    "@angular/platform-browser": "6.0.2",
    "@angular/platform-browser-dynamic": "6.0.2",
    "@angular/router": "6.0.2",
    "core-js": "^2.5.6",
    "@ngxs/devtools-plugin": "^3.0.1",
    "@ngxs/form-plugin": "^3.0.1",
    "@ngxs/logger-plugin": "^3.0.1",
    "@ngxs/store": "^3.0.1",
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular/cli": "^6.0.3",
    "@angular/compiler-cli": "6.0.2",
    "@angular/language-service": "6.0.2",
    "@types/bootstrap": "^3.3.39",
    "@types/electron": "^1.6.10",
    "@types/electron-store": "^1.3.0",
    "@types/faker": "^4.1.2",
    "@types/jasmine": "~2.8.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.3.2",
    "@types/js-yaml": "^3.11.1",
    "@types/lodash": "^4.14.109",
    "@types/node": "^10.1.2",
    "@types/sprintf-js": "^1.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "codelyzer": "^4.3.0",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.0",
    "faker": "^4.1.0",
    "jasmine": "3",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "protractor": "~5.3.2",
    "ts-node": "~6.0.3",
    "tslint": "~5.10.0",
    "typescript": "2.7.2"
  }
}
